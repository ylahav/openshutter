{
  "name": "openshutter",
  "version": "0.9.6",
  "type": "module",
  "description": "A comprehensive photo gallery management system with multi-storage support",
  "main": "index.js",
  "scripts": {
    "dev": "vite dev --port 4000",
    "build": "vite build",
    "preview": "vite preview --port 4000",
    "start": "node build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t openshutter .",
    "docker:run": "docker run -p 4000:4000 --env-file .env.local openshutter",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up -d --build",
    "docker:stop": "docker-compose down && docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:test": "powershell -ExecutionPolicy Bypass -File scripts/test-docker.ps1",
    "migrate:objectid": "node src/scripts/migrate-to-objectid-references.js",
    "migrate:refs": "node src/scripts/migrate-objectid-refs-albums-photos.js"
  },
  "keywords": [
    "photo-gallery",
    "image-management",
    "sveltekit",
    "svelte",
    "nestjs",
    "mongodb",
    "google-drive",
    "aws-s3",
    "typescript"
  ],
  "author": "OpenShutter Team",
  "license": "MIT",
  "dependencies": {
    "@tiptap/core": "^3.13.0",
    "@tiptap/extension-bold": "^3.13.0 ",
    "@tiptap/extension-bullet-list": "^3.13.0 ",
    "@tiptap/extension-code": "^3.13.0 ",
    "@tiptap/extension-color": "^3.13.0 ",
    "@tiptap/extension-hard-break": "^3.13.0 ",
    "@tiptap/extension-heading": "^3.13.0 ",
    "@tiptap/extension-horizontal-rule": "^3.13.0 ",
    "@tiptap/extension-image": "^3.13.0 ",
    "@tiptap/extension-italic": "^3.13.0 ",
    "@tiptap/extension-link": "^3.13.0 ",
    "@tiptap/extension-list-item": "^3.13.0 ",
    "@tiptap/extension-ordered-list": "^3.13.0 ",
    "@tiptap/extension-strike": "^3.13.0 ",
    "@tiptap/extension-text-align": "^3.13.0 ",
    "@tiptap/extension-text-style": "^3.13.0 ",
    "@tiptap/extension-underline": "^3.13.0 ",
    "@tiptap/starter-kit": "^3.13.0 ",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^3.0.3",
    "canvas": "^3.2.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "exif-parser": "^0.1.12",
    "face-api.js": "^0.22.2",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "svelte-dnd-action": "^0.9.68",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "uuid": "^13.0.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/archiver": "^7.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.10",
    "@types/yauzl": "^2.10.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "sass": "^1.99.0",
    "svelte": "^5.45.7",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}

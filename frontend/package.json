{
  "name": "openshutter",
  "version": "0.9.6",
  "type": "module",
  "description": "A comprehensive photo gallery management system with multi-storage support",
  "main": "index.js",
  "scripts": {
    "dev": "vite dev --port 4000",
    "build": "vite build",
    "preview": "vite preview --port 4000",
    "start": "node build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t openshutter .",
    "docker:run": "docker run -p 4000:4000 --env-file .env.local openshutter",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up -d --build",
    "docker:stop": "docker-compose down && docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:test": "powershell -ExecutionPolicy Bypass -File scripts/test-docker.ps1",
    "migrate:objectid": "node src/scripts/migrate-to-objectid-references.js",
    "migrate:refs": "node src/scripts/migrate-objectid-refs-albums-photos.js"
  },
  "keywords": [
    "photo-gallery",
    "image-management",
    "sveltekit",
    "svelte",
    "nestjs",
    "mongodb",
    "google-drive",
    "aws-s3",
    "typescript"
  ],
  "author": "OpenShutter Team",
  "license": "MIT",

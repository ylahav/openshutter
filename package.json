{
  "name": "openshutter",
  "version": "0.9.0",
  "description": "A comprehensive photo gallery management system with multi-storage support",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build:deploy": "node scripts/build-for-deployment.js",
    "docker:build": "chmod +x scripts/docker-build.sh && ./scripts/docker-build.sh",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:deploy": "chmod +x scripts/docker-deploy.sh && ./scripts/docker-deploy.sh",
    "docker:stop": "docker-compose down && docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker logs openshutter-dev || docker logs openshutter-prod"
  },
  "keywords": [
    "photo-gallery",
    "image-management",
    "nextjs",
    "react",
    "mongodb",
    "google-drive",
    "aws-s3",
    "typescript"
  ],
  "author": "OpenShutter Team",
  "license": "MIT",

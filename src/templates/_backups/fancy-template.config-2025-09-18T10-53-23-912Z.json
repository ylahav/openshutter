{
  "templateName": "fancy",
  "displayName": "Fancy",
  "version": "1.0.0",
  "author": "OpenShutter",
  "description": "Elegant template with rich visuals",
  "category": "fancy",
  "features": {
    "responsive": true,
    "darkMode": true,
    "animations": true,
    "seoOptimized": true
  },
  "colors": {
    "primary": "#8B5CF6",
    "secondary": "#0EA5E9",
    "accent": "#F59E0B",
    "background": "#F8FAFC",
    "text": "#0F172A",
    "muted": "#64748B"
  },
  "layout": {
    "maxWidth": "1400px",
    "containerPadding": "2rem",
    "gridGap": "2rem"
  },
  "fonts": {
    "heading": "Inter",
    "body": "Inter"
  },
  "components": {
    "hero": "components/Hero.tsx",
    "navigation": "components/Header.tsx",
    "footer": "components/Footer.tsx"
  },
  "visibility": {
    "hero": true,
    "languageSelector": true,
    "authButtons": true,
    "footerMenu": true
  },
  "pages": {
    "home": "pages/Home.tsx",
    "gallery": "pages/Gallery.tsx",
    "album": "pages/Album.tsx"
  },
  "assets": {
    "thumbnail": "/templates/fancy/thumbnail.jpg"
  }
}